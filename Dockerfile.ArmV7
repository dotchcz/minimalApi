#!/bin/bash
FROM mcr.microsoft.com/dotnet/sdk:6.0-alpine as build
WORKDIR /app
COPY . .
RUN ["dotnet", "restore"]
RUN ["dotnet", "build"]

FROM mcr.microsoft.com/dotnet/aspnet:6.0-alpine as runtime
WORKDIR /app
#!/bin/bash 'COPY --from=build /app/published-app /app'
ENTRYPOINT [ "dotnet", "/app/MinimalApi.dll" ]
